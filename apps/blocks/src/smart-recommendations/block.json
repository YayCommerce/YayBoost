{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 3,
    "name": "yayboost/smart-recommendations",
    "version": "1.0.0",
    "title": "Smart Recommendations",
    "category": "widgets",
    "icon": "star-filled",
    "description": "Display smart recommendations based on customer behavior",
    "keywords": ["yayboost", "smart", "recommendations", "products"],
    "attributes": {
      "queryId": {
        "type": "number"
      },
      "query": {
        "type": "object"
      },
      "displayLayout": {
        "type": "object",
        "default": {
          "type": "flex",
          "columns": 4,
          "shrinkColumns": true
        }
      }
    },
    "providesContext": {
      "queryId": "queryId",
      "query": "query",
      "displayLayout": "displayLayout"
    },
    "supports": {
      "html": false,
      "anchor": true,
      "align": true,
      "spacing": {
        "margin": true,
        "padding": true
      }
    },
    "textdomain": "yayboost",
    "editorScript": "file:./index.js",
    "style": "file:./style-index.css",
    "editorStyle": "file:./style-index.css"
}