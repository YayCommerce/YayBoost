{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "yayboost/frequently-bought-together",
  "version": "1.0.0",
  "title": "Frequently Bought Together",
  "category": "widgets",
  "icon": "cart",
  "description": "Display products that are frequently bought together with real-time selection and batch add to cart",
  "keywords": [
    "yayboost",
    "fbt",
    "frequently bought together",
    "cross-sell",
    "products"
  ],
  "attributes": {
    "queryId": {
      "type": "number"
    },
    "query": {
      "type": "object"
    },
    "displayLayout": {
      "type": "object",
      "default": {
        "type": "flex",
        "columns": 4,
        "shrinkColumns": true
      }
    },
    "currentProductId": {
      "type": "number",
      "default": 0
    }
  },
  "providesContext": {
    "queryId": "queryId",
    "query": "query",
    "displayLayout": "displayLayout",
    "currentProductId": "currentProductId"
  },
  "supports": {
    "html": false,
    "interactivity": true,
    "anchor": true,
    "align": ["wide", "full"],
    "__experimentalLayout": true,
    "spacing": {
      "margin": true,
      "padding": true
    }
  },
  "textdomain": "yayboost",
  "editorScript": "file:./index.js",
  "style": "file:./style-index.css",
  "viewScriptModule": "file:./view.js"
}
